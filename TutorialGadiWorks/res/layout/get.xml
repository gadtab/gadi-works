<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:id="@+id/relLayout">
	<EditText android:layout_width="match_parent"
		android:layout_height="wrap_content" android:id="@+id/etSend"></EditText>
	<Button android:text="StartActivity" android:id="@+id/bSA"
		android:layout_below="@id/etSend" android:layout_alignParentRight="true"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
	<Button android:text="StartActivityForResult" android:id="@+id/bSAFR"
		android:layout_toLeftOf="@id/bSA" android:layout_alignTop="@id/bSA"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
	<TextView android:text="TextView" android:id="@+id/tvGot"
		android:layout_below="@id/bSAFR" android:layout_width="wrap_content"
		android:layout_height="wrap_content"></TextView>
</RelativeLayout>
